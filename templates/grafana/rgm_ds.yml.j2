# {{ ansible_managed }}
# https://grafana.com/docs/grafana/latest/administration/provisioning/#data-sources

apiVersion: 1

datasources:
- name: nagflux
  type: influxdb
  access: proxy
  orgId: 1
  url: http://localhost:8086
  password: '{{ mariadb_pwd }}'
  user: '{{ mariadb_user }}'
  database: 'nagflux'
  basicAuth: false
  basicAuthUser:
  basicAuthPassword:
  withCredentials:
  isDefault: true
  version: 2
  editable: false
- name: Elasticsearch
  type: elasticsearch
  access: proxy
  orgId: 1
  url: http://localhost:9200
  database: 'metricbeat-*'
  basicAuth: false
  isDefault: false
  jsonData:
    esVersion: 70
    #keepCookies: [],
    #logLevelField:
    #logMessageField:
    maxConcurrentShardRequests: 5
    timeField: '@timestamp'
  version: 2
  editable: false
- name: Prometheus
  type: prometheus
  access: proxy
  orgId: 1
  url: http://localhost:9090/prometheus
  database: ''
  basicAuth: false
  isDefault: false
  version: 1
  editable: false
  jsonData:
    httpMethod: GET
    keepCookies: []
